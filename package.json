{
  "name": "credit-rules-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf node_modules doc dist coverage documentation && yarn cache clean",
    "clean-dist": "rimraf dist",
    "clean-coverage": "rimraf coverage",
    "ng": "ng",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start",
    "lint": "ng lint",
    "lint-watch": "npm run lint --force && onchange \"src/**/*.ts\" -- npm run lint --force",
    "e2e": "ng e2e",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet",
    "compodoc": "./node_modules/.bin/compodoc -p ./tsconfig.json --hideGenerator --theme postmark --toggleMenuItems all",
    "build:docs": "npm run compodoc",
    "postinstall": "npm run webdriver-update",
    "build:dev": "ng build --dev --aot --sourcemap --show-circular-dependencies --stats-json --output-hashing all",
    "postbuild:dev": "npm run stats",
    "prebuild:prod": "ng lint",
    "build:prod": "ng build --prod --stats-json",
    "postbuild:prod": "npm run stats",
    "start": "ng serve",
    "test": "ng test --watch=false --code-coverage --browsers ChromeHeadless",
    "test-watch": "ng test",
    "test-linux": "ng test --watch=false --code-coverage --browsers ChromeHeadlessNoSandbox",
    "stats": "./node_modules/.bin/webpack-bundle-analyzer ./dist/stats-es2015.json -m static -r ./coverage/bundles/bundlereport-es2015.html -O",
    "check-before-pr": "npm run clean-dist && npm run clean-coverage && npm run test && npm run build:prod",
    "cbpr": "npm run check-before-pr",
    "sonar": "sonar-scanner"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.11",
    "@angular/common": "^9.1.11",
    "@angular/compiler": "^9.1.11",
    "@angular/core": "^9.1.11",
    "@angular/forms": "^9.1.11",
    "@angular/platform-browser": "^9.1.11",
    "@angular/platform-browser-dynamic": "^9.1.11",
    "@angular/router": "^9.1.11",
    "@esw/styleguide": "^1.4.2",
    "ng-appinsights": "^9.0.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.8",
    "@angular/cli": "^9.1.8",
    "@angular/compiler-cli": "^9.1.11",
    "@angular/language-service": "^9.1.11",
    "@compodoc/compodoc": "^1.1.11",
    "@types/chai": "^4.1.7",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "diff": "^4.0.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.2",
    "karma": "~5.0.9",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-mocha-reporter": "^2.2.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-tfs-reporter": "^1.0.2",
    "mocha": "^6.0.2",
    "onchange": "^7.0.2",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "rxjs-tslint-rules": "^4.33.0",
    "sonar-scanner": "^3.1.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.2",
    "tslint-jasmine-rules": "^1.6.1",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.8.3",
    "webpack-bundle-analyzer": "^3.8.0"
  }
}
